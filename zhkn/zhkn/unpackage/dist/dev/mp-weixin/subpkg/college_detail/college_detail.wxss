.college-box {
  background-color: #ee8367;
  height: 140px;
  display: flex;
  align-items: center;
}
.college-box .college-info {
  display: flex;
  align-items: center;
  margin-left: 20px;
}
.college-box .college-info .logo {
  margin-right: 10px;
  border-radius: 5px;
}
.college-box .college-info .logo image {
  border-radius: 5px;
  width: 50px;
  height: 50px;
}
.college-box .college-info .name {
  color: white;
}
.college-box .college-info .name .type {
  font-size: 12px;
}
.stick-box {
  background-color: white;
  box-shadow: 1px 5px 5px #eee;
  height: 40px;
  position: -webkit-sticky;
  position: sticky;
  top: 0px;
  z-index: 99;
}
.stick-box .stick-info {
  display: flex;
  line-height: 40px;
  justify-content: space-around;
}
.stick-box .stick-info .stick-text {
  height: 80%;
}
.stick-box .stick-info .active {
  color: #ed6c4a;
  border-bottom: 1px solid #ed6c4a;
}
.introduce-box {
  margin-top: 5px;
  box-shadow: 1px 5px 5px #eee;
}
.introduce-box .introduce-title {
  font-weight: bold;
  padding: 10px;
}
.introduce-box .introduce-text {
  padding: 0 10px;
  height: 180rpx;
  overflow: hidden;
}
.introduce-box .overflowActive {
  height: auto;
  overflow: visible;
}
.introduce-box .more {
  text-align: center;
  font-size: 15px;
  color: #ed6c4a;
}
.study-value {
  box-shadow: 1px 5px 5px #eee;
  padding: 10px;
}
.study-value .title {
  margin: 10px 0;
}
.study-value .study-list {
  display: flex;
  flex-wrap: wrap;
}
.study-value .study-list .studyValue-item {
  background-color: #fdf6f4;
  margin: 3px;
  width: 80px;
  height: 80px;
}
.study-value .study-list .studyValue-item .number {
  margin-top: 10px;
  text-align: center;
  font-size: 25px;
  color: #ed6c4a;
  font-weight: bold;
}
.study-value .study-list .studyValue-item .text {
  color: #666666;
  font-size: 13px;
  text-align: center;
}
.score-plan-box .filter-data {
  padding: 10px;
  display: flex;
  align-items: center;
}
.score-plan-box .filter-data .text {
  display: flex;
}
.score-plan-box .filter-data .text .data-item {
  margin-right: 10px;
  font-size: 14px;
}
.score-plan-box .filter-data .text .scorePlanactive {
  color: #ed6c4a;
}
.score-plan-box .filter-data .select {
  display: flex;
}
.score-plan-box .filter-data .select .province-select {
  margin-right: 10px;
}
.score-plan-box .uni-container {
  padding: 10px;
}

